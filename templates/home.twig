<!doctype html>
<html>
    <head>
        <title>{{title}}</title>
        <link rel="stylesheet" href="node_module/bootstrap/dist/css/bootstrap.min.css">
        <script src="node_module/jquery/dist/jquery.min.js"></script>
        <script src="node_module/popper.js/dist/umd/popper.min.js"></script>
        <script src="node_module/bootstrap/dist/js/bootstrap.min.js"></script>
    </head>
    
    <body>
       <main class="container-fluid">
            <div class = "row">
            {% for product in products %}
                <div class="col-md-3"></div> 
                    <img class="img-fluid" src="image/products/products/{{product.image}}">
                    <h4 class="product-name">{{product.name}}</h4>
                    <h6 class="product-price">{{product.price}}</h6>
                    <p class="product-description">{{product.description}}</p>
                </div>
            {% endfor %}
            </div>
       </main>
    </body>
 </html>
























